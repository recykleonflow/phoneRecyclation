    "start": "ng serve --host 178.143.41.216",
    "heroku-postbuild": "nx build client --configuration=prod && nx build $PROJECT_NAME --prod",
    "create-phone-apps": "npx cap sync",
    "prepare": "husky install",
    "deploy:smartcontract": "npx hardhat run apps/smart-contract/hardhat/scripts/deploy.ts --network alfajores",
    "generate:service": "nx g @nrwl/angular:service service_name",
    "typeorm": "ts-node --project ./apps/server/tsconfig.json ./node_modules/typeorm/cli",
    "typeorm:run-migrations": "npm run typeorm migration:run -- -d ./apps/server/src/core/db/typeorm.config.ts",
    "typeorm:generate-migration": "npm run typeorm -- -d ./apps/server/src/core/db/typeorm.config.ts migration:generate ./apps/server/src/migrations/$npm_config_name",
    "typeorm:create-migration": "npm run typeorm -- migration:create ./apps/server/src/migrations/$npm_config_name",
    "typeorm:revert-migration": "npm run typeorm -- -d ./apps/server/src/core/db/typeorm.config.ts migration:revert"
  },
